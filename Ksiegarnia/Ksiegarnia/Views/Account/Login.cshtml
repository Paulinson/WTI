@{
    ViewBag.Title = "Log in";
}

@model string

<div class="container">
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
        
            <div class="account-wall">
            
                <form class="form-signin">
                    <input type="text" class="form-control" placeholder="Wpisz Email" required autofocus>
                    <input type="password" class="form-control" placeholder="Wpisz Hasło" required>
                    <button class="btn btn-lg btn-primary btn-block" type="submit">
                        Zaloguj się
                    </button>
                   
                </form>
            </div>
        </div>
    </div>
</div>